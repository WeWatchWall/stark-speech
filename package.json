{
  "name": "stark-speech",
  "version": "1.0.4",
  "description": "Easy to integrate speech functions for browsers.",
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeWatchWall/stark-speech.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "ISC",
  "keywords": [
    "stark",
    "speech",
    "synthesis",
    "voice",
    "recognition"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "node clean.js dist build package",
    "build-all": "npm run clean && npm install",
    "postinstall": "npx patch-package && node build.js",
    "updatePublish": "node publish.js"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "latest-version": "^6.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@tensorflow-models/speech-commands": "0.3.9",
    "@tensorflow/tfjs": "1.7.0",
    "annyang": "^2.6.1",
    "esbuild": "^0.14.30",
    "flat-promise": "^1.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-debugger": 1
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
